#
# Calculator
# 

sum = 0

# 1. Input

x1 = input("Enter x1: ")
x2 = input("Enter x2: ")
# x3 = input("Enter x3: ")
op = input("Enter operator: ")

# 2. Process

if op == "+":
    sum = int(x1) + int(x2)
elif op == "-":
    sum = int(x1) - int(x2)
# elif op == "*":
#     sum = int(x1) * int(x2)
# elif op == "/":
#     sum = int(x1) / int(x2)
# elif op == "avg":
#     sum = (int(x1) + int(x2))/2
# elif op == "sd":
    
# 3. Output

print(f"Sum: {sum}")
